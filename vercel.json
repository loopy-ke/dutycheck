{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.dutycheck.co.ke" }],
      "destination": "https://dutycheck.co.ke/$1",
      "permanent": true
    }
  ]
}
